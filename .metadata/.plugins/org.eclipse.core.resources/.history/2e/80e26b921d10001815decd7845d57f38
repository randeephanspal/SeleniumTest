package InterviewQs;

public class SwapStrings {

	//swap string without using third variable
	
	public static void main(String[] args) {


		String a = "Hello";
		String b = "World";
		
		a = a+b; //HelloWorld
		b = a.substring(0, a.length()-b.length());
		System.out.println(b);
		a = a.substring(b.length(), 10);
		System.out.println(a);
		
	}

}
